<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DashedProgressBar">
        <attr name="filledProgressBarLength" format="dimension" />
        <attr name="filledProgressBarWidth" format="dimension" />
        <attr name="filledProgressBarSpacing" format="dimension" />
        <attr name="filledProgressBarColor" format="color" />

        <attr name="emptyProgressBarLength" format="dimension" />
        <attr name="emptyProgressBarWidth" format="dimension" />
        <attr name="emptyProgressBarSpacing" format="dimension" />
        <attr name="emptyProgressBarColor" format="color" />

        <attr name="startAngle" format="float" />
        <attr name="endAngle" format="float" />

        <attr name="direction" format="enum">
            <enum name="clockwise" value="1" />
            <enum name="counterclockwise" value="-1" />
        </attr>

        <attr name="cutPadding" format="boolean" />

        <attr name="bg_rim_width" format="dimension" />
        <attr name="bg_rim_color" format="color" />
    </declare-styleable>

</resources>